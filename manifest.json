{
  "manifest_version": 3,
  "name": "Tab Title Reader",
  "version": "1.0",
  "permissions": [
      "tabs",
      "downloads",
      "activeTab",
      "scripting"
  ],
  "host_permissions": [
      "http://localhost:3000/*"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  }
}
